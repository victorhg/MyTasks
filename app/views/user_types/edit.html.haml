.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", user_types_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_user_type_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_user_type_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      UserType
    .inner
      = form_for @user_type, :url => user_type_path(@user_type), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
