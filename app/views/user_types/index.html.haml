.block
  .secondary-navigation
    %ul.wat-cf
      %li.first.active= link_to "#{t("web-app-theme.list", :default => "List")}", user_types_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_user_type_path
  .content
    %h2.title
      = t("web-app-theme.all", :default => "All")
      UserTypes
    .inner
      %table.table
        %tr
          %th.first ID
          %th
            = t("activerecord.attributes.user_type.name", :default => t("activerecord.labels.name", :default => "Name"))
          %th= t("web-app-theme.created_at", :default => "Created at")
          %th.last &nbsp;
        - @user_types.each do |user_type|
          %tr{:class => cycle("odd", "even")}
            %td
              = user_type.id
            %td
              = link_to user_type.name, user_type_path(user_type)
            %td
              = user_type.created_at
            %td.last
              = link_to "#{t("web-app-theme.show", :default => "Show")}", user_type_path(user_type)
              |
              \#{link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_user_type_path(user_type)} |
              \#{link_to "#{t("web-app-theme.delete", :default => "Delete")}", user_type_path(user_type), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}"}
      .actions-bar.wat-cf
        .actions
        = paginate @user_types
- content_for :sidebar, render(:partial => 'sidebar')
