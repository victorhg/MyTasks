.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", appconfigs_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_appconfig_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_appconfig_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      Appconfig
    .inner
      = form_for @appconfig, :url => appconfig_path(@appconfig), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
